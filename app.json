{
  "expo": {
    "name": "ubicard",
    "slug": "ubicard",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/oyster.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/oyster.png",
      "resizeMode": "contain",
      "backgroundColor": "#151515"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/oyster.png",
        "backgroundColor": "#151515"
      }
    },
    "web": {
      "favicon": "./assets/oyster.png",
      "bundler": "metro"
    }
  }
}
